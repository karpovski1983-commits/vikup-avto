    <script>
        // Mobile menu toggle
        function toggleMenu() {
            const headerWrapper = document.getElementById('headerWrapper');
            headerWrapper.classList.toggle('active');
        }

        // Toggle brands
        function toggleBrands() {
            const hiddenBrands = document.getElementById('hiddenBrands');
            const brandsGrid = document.getElementById('brandsGrid');
            const button = document.getElementById('showMoreBrands');
            
            if (hiddenBrands.classList.contains('show')) {
                hiddenBrands.classList.remove('show');
                brandsGrid.classList.remove('show');
                button.textContent = '–ü–æ–∫–∞–∑–∞—Ç—å –≤—Å–µ –º–∞—Ä–∫–∏ ‚ñæ';
            } else {
                hiddenBrands.classList.add('show');
                brandsGrid.classList.add('show');
                button.textContent = '–°–∫—Ä—ã—Ç—å –º–∞—Ä–∫–∏ ‚ñ¥';
            }
        }

        // FAQ Accordion
        document.querySelectorAll('.faq-question').forEach(question => {
            question.addEventListener('click', () => {
                const item = question.parentElement;
                const isActive = item.classList.contains('active');
                
                document.querySelectorAll('.faq-item').forEach(faq => {
                    faq.classList.remove('active');
                });
                
                if (!isActive) {
                    item.classList.add('active');
                }
            });
        });

        // Form submissions
        document.querySelectorAll('form').forEach(form => {
            form.addEventListener('submit', function(e) {
                e.preventDefault();
                alert('–°–ø–∞—Å–∏–±–æ –∑–∞ –∑–∞—è–≤–∫—É! –ú—ã —Å–≤—è–∂–µ–º—Å—è —Å –≤–∞–º–∏ –≤ –±–ª–∏–∂–∞–π—à–µ–µ –≤—Ä–µ–º—è.');
                this.reset();
            });
        });

        // Counter animation
        const counter = document.querySelector('.counter');
        if (counter) {
            const target = 1347;
            let count = 0;
            const increment = target / 100;

            const updateCounter = () => {
                if (count < target) {
                    count += increment;
                    counter.innerHTML = Math.ceil(count) + '<div class="counter-label">–º–∞—à–∏–Ω –≤—ã–∫—É–ø–ª–µ–Ω–æ –Ω–∞–º–∏</div>';
                    setTimeout(updateCounter, 20);
                } else {
                    counter.innerHTML = target + '<div class="counter-label">–º–∞—à–∏–Ω –≤—ã–∫—É–ø–ª–µ–Ω–æ –Ω–∞–º–∏</div>';
                }
            };

            window.addEventListener('load', updateCounter);
        }
    </script>
</body>
</html>
